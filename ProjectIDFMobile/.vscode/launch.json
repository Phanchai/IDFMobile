{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET MAUI iOS",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0-ios/iossimulator-arm64/ProjectIDFMobile.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "SIMULATOR_UDID": "DDD3D426-1DA9-4AFE-B5C0-C1D8D28BAC70"
            }
        },
        {
            "name": ".NET MAUI MacCatalyst Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build MacCatalyst",
            "program": "${workspaceFolder}/bin/Debug/net8.0-maccatalyst/ProjectIDFMobile.app/Contents/MacOS/ProjectIDFMobile",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}
